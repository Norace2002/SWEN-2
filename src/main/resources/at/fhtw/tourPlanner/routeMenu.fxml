<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhtw.tourPlanner.view.RouteMenuController">
  <columnConstraints>
    <ColumnConstraints hgrow="ALWAYS" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="34.0" minHeight="34.0" prefHeight="34.0" vgrow="NEVER" />
    <RowConstraints minHeight="50.0" vgrow="ALWAYS" />
  </rowConstraints>
   <children>
      <ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="34.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="NEVER">
        <buttons>
            <Button fx:id="routeButton" mnemonicParsing="false" onAction="#loadRouteWindow" text="Route" />
            <Button fx:id="generalButton" mnemonicParsing="false" onAction="#loadGeneralWindow" text="General" />
        </buttons>
      </ButtonBar>
      <AnchorPane fx:id="contentWindowPane" prefHeight="200.0" prefWidth="200.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" />
   </children>
</GridPane>
